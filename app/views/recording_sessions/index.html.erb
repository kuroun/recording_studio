<h1>Listing recording_sessions</h1>

<table>
  <thead>
    <tr>
      <th>Start time</th>
      <th>End time</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @recording_sessions.each do |recording_session| %>
      <tr>
        <td><%= recording_session.start_time %></td>
        <td><%= recording_session.end_time %></td>
        <td><%= recording_session.description %></td>
        <td><%= link_to 'Show', recording_session %></td>
        <td><%= link_to 'Edit', edit_recording_session_path(recording_session) %></td>
        <td><%= link_to 'Destroy', recording_session, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Recording session', new_recording_session_path %>
